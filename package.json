{
  "name": "new-nodejs-app",
  "version": "1.0.0",
  "description": "NodeJs REST API",
  "main": "app.js",
  "scripts": {
    "test": "jest --collect-coverage",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "clean:dist": "rm -rf dis && mkdir dist",
    "build-server": "babel -d ./dist ./src",
    "build": "npm run clean:dist && npm run build-server",
    "start": "NODE_ENV=development nodemon --exec babel-node ./src/app.js"
  },
  "keywords": [
    "NodeJS",
    "generate-node-project"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.7.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "nodemon": "^2.0.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  }
}
